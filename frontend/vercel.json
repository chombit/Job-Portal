{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm install --no-fund --no-audit --prefer-offline && npm run build",
  "installCommand": "npm install --no-fund --no-audit --prefer-offline",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ]
}